<Window x:Class="HellPie.Revit.PrismDemo.Views.ShellPrismWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:ctrl="clr-namespace:HellPie.Revit.PrismDemo.Views.Common"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:vm="clr-namespace:HellPie.Revit.PrismDemo.ViewModels"
        mc:Ignorable="d"
        Title="{Binding Title, UpdateSourceTrigger=PropertyChanged}"
        Height="400"
        Width="550"
        ResizeMode="NoResize"
        Topmost="True"
        prism:ViewModelLocator.AutoWireViewModel="True"
        d:DataContext="{vm:ShellPrismWindowViewModel}">
    <Grid>
        <ctrl:PrismTextBox />
    </Grid>
</Window>
